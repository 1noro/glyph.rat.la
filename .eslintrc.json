{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console": ["error", { "allow": ["log", "warn", "error"] }],
        "indent": ["error", 4],
        "import/extensions": ["error", "always"]
    }
}
